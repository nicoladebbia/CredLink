{
  "name": "@c2pa/sw-relay",
  "version": "1.0.0",
  "description": "C2PA Service Worker for CORS/Mixed Content Relay",
  "main": "dist/sw.js",
  "types": "dist/sw.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && rollup -c",
    "build:sw": "tsc sw.ts --target ES2020 --lib ES2020,WebWorker --outDir dist --moduleResolution node",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "c2pa",
    "service-worker",
    "cors",
    "manifest",
    "relay"
  ],
  "author": "C2PA Team",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.37.0",
    "jest": "^29.5.0",
    "rollup": "^3.20.2",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.2"
  },
  "dependencies": {},
  "peerDependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/c2pa/credlink.git",
    "directory": "packages/sw-relay"
  },
  "bugs": {
    "url": "https://github.com/c2pa/credlink/issues"
  },
  "homepage": "https://github.com/c2pa/credlink#readme"
