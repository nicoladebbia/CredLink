{
  "name": "c2pa-reverse-lab",
  "version": "1.1.0",
  "description": "Optimizer Behavior Reverse-Lab - Fingerprint and track CDN/optimizer behavior for C2PA preservation",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "run-matrix": "tsx src/cli/run-matrix.ts",
    "profile": "tsx src/cli/run-matrix.ts profile-provider"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^9.0.1",
    "@fastify/swagger": "^8.12.0",
    "@fastify/swagger-ui": "^2.0.0",
    "fastify": "^4.24.3",
    "pino": "^10.1.0",
    "pino-pretty": "^10.2.3",
    "ioredis": "^5.3.2",
    "node-cron": "^3.0.3",
    "zod": "^3.22.4",
    "nanoid": "^5.0.4",
    "cheerio": "^1.0.0-rc.12",
    "json5": "^2.2.3",
    "semver": "^7.5.4",
    "helmet": "^7.1.0",
    "joi": "^17.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.54.0",
    "tsx": "^4.1.4",
    "typescript": "^5.2.2",
    "tsc-alias": "^1.8.8",
    "vitest": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "@vitest/coverage-v8": "^0.34.6"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "c2pa",
    "cai",
    "content-credentials",
    "optimizer",
    "cdn",
    "reverse-lab",
    "behavior-fingerprinting"
  ],
  "author": "C2PA Audit Team",
  "license": "MIT"
}
