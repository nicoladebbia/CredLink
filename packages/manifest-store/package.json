{
  "name": "@credlink/manifest-store",
  "version": "0.1.0",
  "description": "Remote Manifest Store v1 - Write-once, hash-addressed manifest storage",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "node --test dist/**/*.test.js",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@credlink/utils": "file:../utils"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240512.0",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  }
}
