  "compilerOptions": {
    "outDir": "./dist",
    "rootDir": "./",
    "module": "CommonJS",
    "target": "ES2022",
    "moduleResolution": "node",
    "allowJs": true,
    "checkJs": false,
    "noEmit": false,
    "declaration": false,
    "sourceMap": false
  },
  "include": [
    "server.js"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
