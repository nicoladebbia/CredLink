{
  "name": "@c2pa/analytics",
  "version": "1.0.0",
  "description": "Analytics SKU v1 - SLO monitoring, dashboards, and survival reports",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node dist/index.js",
    "ingest": "ts-node src/ingest/run.ts",
    "alerts": "ts-node src/alerts/run.ts",
    "reports": "ts-node src/reports/build.ts",
    "test": "jest",
    "test:integration": "jest --testPathPattern=integration",
    "test:acceptance": "jest --testPathPattern=acceptance"
  },
  "dependencies": {
    "fastify": "^4.15.0",
    "@fastify/cors": "^8.2.0",
    "@fastify/static": "^6.5.0",
    "@clickhouse/client": "^0.2.0",
    "@aws-sdk/client-s3": "^3.300.0",
    "handlebars": "^4.7.7",
    "csv-writer": "^1.6.0",
    "nodemailer": "^6.9.0",
    "slack-web-api": "^6.8.0",
    "cron": "^2.3.0",
    "yaml": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.0",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@types/node": "^18.15.0",
    "@types/jest": "^29.5.0",
    "@types/nodemailer": "^6.4.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.2"
  }
}
