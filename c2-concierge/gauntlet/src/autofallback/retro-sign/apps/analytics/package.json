{
  "name": "@c2pa/analytics",
  "version": "1.0.0",
  "description": "Analytics SKU v1 - SLO monitoring, dashboards, and survival reports",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "security:attacks": "ts-node security/runners/run_attacks.ts",
    "security:fuzz": "ts-node security/runners/run_fuzz.ts",
    "security:chaos": "ts-node security/runners/run_chaos.ts",
    "security:parity": "ts-node security/runners/run_parity.ts",
    "security:stabilization": "ts-node security/runners/run_stabilization.ts",
    "security:all": "npm run security:attacks && npm run security:fuzz && npm run security:chaos && npm run security:parity && npm run security:stabilization",
    "docker:build": "docker build -t c2pa-analytics .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down"
  },
  "dependencies": {
    "fastify": "^4.21.0",
    "@fastify/cors": "^8.4.0",
    "@fastify/static": "^6.12.0",
    "@clickhouse/client": "^0.2.7",
    "@aws-sdk/client-s3": "^3.370.0",
    "handlebars": "^4.7.7",
    "csv-writer": "^1.6.0",
    "nodemailer": "^6.9.8",
    "@slack/web-api": "^6.9.0",
    "node-cron": "^3.0.2",
    "yaml": "^2.3.2",
    "jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.1",
    "winston": "^3.10.0",
    "joi": "^17.9.2",
    "uuid": "^9.0.0",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.10.0"
  },
  "devDependencies": {
    "@types/node": "^20.5.0",
    "@types/jest": "^29.5.4",
    "@types/nodemailer": "^6.4.9",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/bcrypt": "^5.0.0",
    "@types/uuid": "^9.0.2",
    "@types/node-cron": "^3.0.8",
    "@types/joi": "^17.2.3",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "prettier": "^3.0.2",
    "jest": "^29.6.2",
    "jest-junit": "^15.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  }
}
